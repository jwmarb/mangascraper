{
  "name": "@specify_/mangascraper",
  "version": "3.4.4",
  "description": "A mangascraper for fetching manga from a variety of manga sources (mangakakalot, manganato, and etc.)",
  "private": false,
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "build": "tsc -p .",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "scraper",
    "manga",
    "anime",
    "manhua",
    "manhwa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EGGaming/mangascraper.git"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jquery": "^3.5.5",
    "@types/random-useragent": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-tsdoc": "^0.2.14",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "puppeteer": "^10.1.0",
    "typescript": "^4.3.4"
  },
  "peerDependencies": {
    "puppeteer": "*"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.10",
    "date-fns": "^2.22.1",
    "jquery": "^3.6.0",
    "random-useragent": "^0.5.0"
  }
}
